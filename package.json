{
  "name": "sag-mongodb",
  "version": "1.0.0",
  "type": "module",
  "description": "Silent Auction Gallery with a MongoDB back end.",
  "main": "server.js",
  "scripts": {
    "ngrok": "nodemon -r dotenv/config server.js & e:\ngrok-v3\ngrok http http://localhost:3000 ",
    "start": "npm run lint & nodemon -r dotenv/config server.js",
    "test": "npm run lint",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "lint:safe": "eslint ./ --fix --quiet"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jquery": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "no-trailing-spaces": "error",
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-console": "warn",
      "no-undef": "warn",
      "consistent-return": "warn",
      "no-restricted-syntax": "warn",
      "no-else-return": "warn",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^M"
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deawar/SAG-MongoDB.git"
  },
  "keywords": [
    "MongoDB",
    "Mongoose",
    "Express",
    "Node",
    "Express-Handlebars"
  ],
  "author": "Dean Warren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deawar/SAG-MongoDB/issues"
  },
  "homepage": "https://github.com/deawar/SAG-MongoDB#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "bson": "^6.10.1",
    "chalk": "^4.1.2",
    "cloudinary": "^1.41.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-flash": "^0.0.2",
    "express-flash-notification": "^0.5.0",
    "express-handlebars": "^5.3.1",
    "express-session": "^1.17.1",
    "fix": "^0.0.6",
    "formidable": "^1.2.2",
    "mongodb": "^3.6.4",
    "mongoose": "^8.10.0",
    "mongoose-type-phone": "^1.0.1",
    "mongoose-unique-validator": "^4.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.10.0",
    "os": "^0.1.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "randomstring": "^1.3.1",
    "twilio": "^3.84.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "nodemon": "^2.0.22"
  }
}
